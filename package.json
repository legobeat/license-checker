    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": "^12.13.0 || >=14.15.0"
  },
  "nyc": {
    "check-coverage": true,
    "exclude": [
      "**/tests/*.js"
    ],
    "lines": 99,
    "statements": 99,
    "functions": 100,
    "branches": 99
  }
